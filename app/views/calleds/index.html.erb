<h1>Listing Calleds</h1>

<table class="responsive">
  <thead>
    <tr>
      <th>System</th>
      <th>Tipo</th>
      <th>Identificador</th>
      <th>Data</th>
      <th>Descricao</th>
      <th colspan="3">Actions</th>
    </tr>
 </thead>
  <tbody>
    <% @calleds.each do |called| %>
      <tr>
        <td><%= called.system.nome %></td>
        <td><%= called.formata_tipo(called.tipo) %></td>
        <td><%= called.identificador %></td>
        <td><%= formata_data(called.data) %></td>
        <td><%= called.descricao %></td>
        <td><%= link_to 'Show', called %></td>
        <td><%= link_to 'Edit', edit_called_path(called) %></td>
        <td><%= link_to 'Destroy', called, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Called', new_called_path %>
